@{ Layout = "_Layout"; }
@using MtgApiManager.Lib.Service;
@using MtgApiManager.Lib.Core;
@using MtgApiManager.Lib.Model;



@if(@Model.Count == 0)
{
    <h1>I'm sorry, I have nothing to show you</h1>
}

@if(@Model.Count > 0)
{
    <div class="grid-container">

        @foreach(string img in @Model)
        {
            <img src="@img"/>

        }

    </div>

}